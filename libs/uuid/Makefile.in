DEFS      = @DLLFLAGS@ -D__WINESRC__
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = libwine_uuid.a

C_SRCS = \
	dx8guid.c \
	dxguid.c \
	uuid.c

all: $(MODULE)

@MAKE_RULES@

$(MODULE): $(OBJS)
	$(RM) $@
	$(AR) $@ $(OBJS)
	$(RANLIB) $@

install:: libwine_uuid.a
	$(MKINSTALLDIRS) $(libdir)
	$(INSTALL_DATA) libwine_uuid.a $(libdir)/libwine_uuid.a

uninstall::
	$(RM) $(libdir)/libwine_uuid.a

### Dependencies:

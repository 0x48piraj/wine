install:: $(FONTS)
	$(MKINSTALLDIRS) $(fontdir)
	for i in $(FONTS); do $(INSTALL_DATA) $$i $(fontdir)/$$i || exit 1; done

uninstall::
	-cd $(fontdir) && $(RM) $(FONTS)
	-rmdir $(fontdir)

clean::
	$(RM) *.fon *.fnt *.ttf


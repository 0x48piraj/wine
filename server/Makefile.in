DEFS      = @DLLFLAGS@ -D__WINE__ -D__WINE_SERVER__
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = server

C_SRCS = \
	change.c \
	console.c \
	debugger.c \
	device.c \
	event.c \
	file.c \
	handle.c \
	mapping.c \
	mutex.c \
	object.c \
	pipe.c \
	process.c \
	request.c \
	snapshot.c \
	select.c \
	semaphore.c \
	socket.c \
	thread.c \
	trace.c

EXTRA_SRCS = main.c
MAIN_OBJS = main.o

PROGRAMS = wineserver

all: $(MODULE).o $(PROGRAMS)

@MAKE_RULES@

wineserver: $(OBJS) $(MAIN_OBJS)
	$(CC) -o $(PROGRAMS) $(OBJS) $(MAIN_OBJS) $(LIBS)


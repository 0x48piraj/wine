TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = ne

C_SRCS = \
	convert.c \
	module.c \
	resource.c \
	segment.c

GLUE = module.c segment.c

all: $(MODULE).o

@MAKE_RULES@

$(MODULE).o: $(OBJS) Makefile.in $(TOPSRCDIR)/Make.rules.in
	$(LDCOMBINE) $(OBJS) -o $@


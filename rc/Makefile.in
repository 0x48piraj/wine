DEFS      = -D__WINE__ -DNO_TRANSITION_TYPES
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
PROGRAMS  = winerc@PROGEXT@
MODULE    = none

C_SRCS     = winerc.c
GEN_C_SRCS = y.tab.c lex.yy.c

all: $(PROGRAMS)

@MAKE_RULES@

$(PROGRAMS): $(OBJS)
	$(CC) $(CFLAGS) -o winerc@PROGEXT@ $(OBJS) $(LEXLIB)

y.tab.c y.tab.h: parser.y
	$(YACC) -d -t $(SRCDIR)/parser.y

lex.yy.c: parser.l
	$(LEX) -8 -I $(SRCDIR)/parser.l

### Dependencies:

DEFS      = -D__WINE__ -DNO_TRANSITION_TYPES
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
PROGRAM   = winerc
MODULE    = none

C_SRCS     = winerc.c
GEN_C_SRCS = y.tab.c lex.yy.c

all: $(PROGRAM)

@MAKE_RULES@

$(PROGRAM): $(OBJS)
	$(CC) $(CFLAGS) -o winerc $(OBJS) $(LEXLIB)

y.tab.c y.tab.h: parser.y
	$(YACC) -d -t $(SRCDIR)/parser.y

lex.yy.c: parser.l
	$(LEX) -8 -I $(SRCDIR)/parser.l

clean::
	$(RM) y.tab.h $(PROGRAM)

### Dependencies:

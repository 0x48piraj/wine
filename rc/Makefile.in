DEFS   = -D__WINE__ -DNO_TRANSITION_TYPES
TOPSRC = @top_srcdir@

PROGRAM = winerc
MODULE = none

C_SRCS = y.tab.c lex.yy.c winerc.c 

all: $(PROGRAM)

@MAKE_RULES@

$(PROGRAM): $(OBJS)
	$(CC) $(CFLAGS) -o winerc $(OBJS) $(LEXLIB)

y.tab.c y.tab.h: parser.y
	$(YACC) -d -t parser.y

lex.yy.c: parser.l
	$(LEX) -8 -I parser.l

clean::
	$(RM) y.tab.c y.tab.h lex.yy.c $(PROGRAM)

### Dependencies:

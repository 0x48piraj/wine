DEFS      = @DLLFLAGS@ -D__WINE__
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none

C_SRCS = \
	uuid.c

all: libwine_uuid.a

@MAKE_RULES@

libwine_uuid.a: uuid.o
	$(RM) $@
	$(AR) $@ uuid.o
	$(RANLIB) $@

install:: libwine_uuid.a
	$(MKINSTALLDIRS) $(libdir)
	$(INSTALL_DATA) libwine_uuid.a $(libdir)/libwine_uuid.a

uninstall::
	$(RM) $(libdir)/libwine_uuid.a

### Dependencies:

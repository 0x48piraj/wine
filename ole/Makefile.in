DEFS      = @DLLFLAGS@ -D__WINE__
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none

C_SRCS = \
	dx8guid.c \
	dxguid.c \
	uuid.c

all: libwine_uuid.a

@MAKE_RULES@

libwine_uuid.a: $(OBJS)
	$(RM) $@
	$(AR) $@ $(OBJS)
	$(RANLIB) $@

install:: libwine_uuid.a
	$(MKINSTALLDIRS) $(libdir)
	$(INSTALL_DATA) libwine_uuid.a $(libdir)/libwine_uuid.a

uninstall::
	$(RM) $(libdir)/libwine_uuid.a

### Dependencies:

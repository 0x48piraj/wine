DEFS      = -DWINELIB
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none
PROGRAMS  = regapi

C_SRCS = \
	regapi.c

SPEC_SRCS = \
	regapi.spec

all: $(PROGRAMS)

@MAKE_RULES@

regapi: $(OBJS)
	$(CC) -o regapi $(OBJS) $(DLL_LINK) $(LIBS)

install::
	$(INSTALL_PROGRAM) regapi $(bindir)/regapi

uninstall::
	$(RM) $(bindir)/regapi

### Dependencies:

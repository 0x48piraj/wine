DEFS      = -DWINELIB
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none
PROGRAMS  = regapi
ALL_LIBS  = $(WINELIB) $(X_LIBS) $(XLIB) $(LIBS)
RCFLAGS   = -w32 -h
WRCEXTRA  = -t -A -p $*

C_SRCS = \
	regapi.c

all: $(PROGRAMS)

@MAKE_RULES@

regapi: $(OBJS) $(WINESTUB)
	$(CC) -o regapi $(OBJS) $(LDOPTIONS) $(ALL_LIBS)

install::
	$(INSTALL_PROGRAM) regapi $(bindir)/regapi

uninstall::
	$(RM) $(bindir)/regapi

### Dependencies:

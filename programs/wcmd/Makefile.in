DEFS      = -DWINELIB
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none
PROGRAMS  = wcmd
ALL_LIBS  = $(WINELIB) $(X_LIBS) $(XLIB) $(LIBS)
WRCEXTRA  = -s -p wcmd

C_SRCS = \
	batch.c \
	builtins.c \
	directory.c \
	wcmdmain.c

SPEC_SRCS = \
	wcmd.spec

RC_SRCS = \
	wcmdrc.rc

all: check_wrc $(PROGRAMS)

@MAKE_RULES@

#this line is needed to prevent winestub.o being linked
WINESTUB = 

wcmd: $(OBJS)
	$(CC) -o wcmd $(OBJS) $(LDOPTIONS) $(ALL_LIBS)

install::
	$(INSTALL_PROGRAM) wcmd $(bindir)/wcmd

uninstall::
	$(RM) $(bindir)/wcmd

### Dependencies:

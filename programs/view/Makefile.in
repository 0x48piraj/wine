DEFS      = -DWINELIB
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none
PROGRAMS  = view
ALL_LIBS  = $(WINELIB) $(X_LIBS) $(XLIB) $(LIBS)
RCFLAGS   = -w32 -h
WRCEXTRA  = -t -A -p $*

C_SRCS = \
	init.c \
	view.c \
	winmain.c

RC_SRCS = \
	viewrc.rc

all: check_wrc $(PROGRAMS)

depend:: $(RC_SRCS:.rc=.h)

@MAKE_RULES@

# Override resource compiler rules
.rc.s:
	$(CPP) $(DEFS) $(OPTIONS) $(DIVINCL) -DRC_INVOKED -P -x c $< | $(WRC) $(WRCFLAGS) $(WRCEXTRA) -o $*.s

.rc.h:
	$(CPP) $(DEFS) $(OPTIONS) $(DIVINCL) -DRC_INVOKED -P -x c $< | $(WRC) $(WRCFLAGS) $(WRCEXTRA) -nH $*.h


view: $(OBJS)
	$(CC) -o view $(OBJS) $(LDOPTIONS) $(ALL_LIBS)

install: dummy
	$(INSTALL_PROGRAM) view $(bindir)/view

uninstall: dummy
	$(RM) $(bindir)/view

$(RC_SRCS:.rc=.s): $(WRC)

dummy:


DEFS      = -DWINELIB
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none
PROGRAMS  = view
ALL_LIBS  = $(WINELIB) $(X_LIBS) $(XPM_LIB) $(XLIB) $(LIBS)
RCFLAGS   = -w32 -h

C_SRCS = \
	init.c \
	view.c \
	winmain.c

RC_SRCS = \
	viewrc.rc

all: check_winerc $(PROGRAMS)

depend:: $(RC_SRCS:.rc=.h)

@MAKE_RULES@

view: $(OBJS)
	$(CC) -o view $(OBJS) $(LDOPTIONS) $(ALL_LIBS)

install: dummy
	$(INSTALL_PROGRAM) view $(bindir)/view

$(RC_SRCS:.rc=.c) $(RC_SRCS:.rc=.h): $(WINERC)

dummy:


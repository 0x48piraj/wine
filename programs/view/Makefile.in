DEFS      = -DWINELIB
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none
PROGRAMS  = view
ALL_LIBS  = $(WINELIB) $(X_LIBS) $(XLIB) $(LIBS)
WRCEXTRA  = -s -p view

C_SRCS = \
	init.c \
	view.c \
	winmain.c

SPEC_SRCS = \
	view.spec

RC_SRCS = \
	viewrc.rc

all: check_wrc $(PROGRAMS)

@MAKE_RULES@

view: $(OBJS)
	$(CC) -o view $(OBJS) $(LDOPTIONS) $(ALL_LIBS)

install::
	$(INSTALL_PROGRAM) view $(bindir)/view

uninstall::
	$(RM) $(bindir)/view

### Dependencies:

#!/bin/sh

if [ -z "$WINE_BUILD" ]; then
    WINE_BUILD="`date +%Y%m%d.%H%M-auto`"
    echo "warning: using automatically generated BUILD tag: $WINE_BUILD" 1>&2
fi

echo "/* Automatically generated -- do not edit! */"
echo "STRINGTABLE {"
echo "0 \"$WINE_BUILD\""

i=0
for test
do
    i=`expr $i + 1`
    echo "$i \"$test\""
done

if [ -f revision.info ]; then
    i=0
    for line in `cat revision.info`; do
        i=`expr $i + 1`
	echo "$i+30000 \"$line\""
    done
fi

echo "}"

i=0
for test
do
    i=`expr $i + 1`
    echo "$i USERDATA \"$test\""
done

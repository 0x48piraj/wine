DEFS      = -DWINELIB -DSTRICT
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none
PROGRAMS  = progman
IMPORTS   = shlwapi shell32 comdlg32 ole32 rpcrt4 winspool.drv user32 gdi32 kernel32
WRCEXTRA  = -s -p progman

LICENSELANG = En

C_SRCS = \
	dialog.c \
	group.c \
	grpfile.c \
	license.c \
	main.c \
	program.c \
	string.c \
	$(LICENSELANG:%=License_%.c)

SPEC_SRCS = progman.spec

RC_SRCS = rsrc.rc

all: check_wrc $(PROGRAMS)

@MAKE_RULES@

progman: $(OBJS)
	$(CC) -o progman $(OBJS) $(DLL_LINK) $(LIBS)

install::
	$(INSTALL_PROGRAM) progman $(bindir)/progman

uninstall::
	$(RM) $(bindir)/progman

### Dependencies:

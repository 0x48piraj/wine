DEFS      = -DWINELIB
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none
PROGRAMS  = winemine
WRCEXTRA  = -s -p winemine

LICENSELANG = En

C_SRCS = \
	main.c \
        dialog.c


SPEC_SRCS = winemine.spec

RC_SRCS = rsrc.rc

all: check_wrc $(PROGRAMS)

@MAKE_RULES@

# Override resource compiler rules
.rc.s:
	$(CPP) $(DEFS) $(OPTIONS) $(DIVINCL) -DRC_INVOKED -P -x c $< | $(WRC) $(WRCFLAGS) $(WRCEXTRA) -o $*.s

winemine: $(OBJS)
	$(CC) -o winemine $(OBJS) $(DLL_LINK) $(LIBS)

install::
	$(INSTALL_PROGRAM) winemine $(bindir)/winemine

uninstall::
	$(RM) $(bindir)/winemine

### Dependencies:

DEFS      = -DWINELIB
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none
PROGRAMS  = regtest
ALL_LIBS  = $(WINELIB) $(X_LIBS) $(XLIB) $(LIBS)
RCFLAGS   = -w32 -h
WRCEXTRA  = -t -A -p $*

C_SRCS = regtest.c

all: $(PROGRAMS)

@MAKE_RULES@

regtest: $(OBJS) $(WINESTUB)
	$(CC) -o regtest $(OBJS) $(LDOPTIONS) $(ALL_LIBS)

install::
	$(INSTALL_PROGRAM) regtest $(bindir)/regtest

uninstall::
	$(RM) $(bindir)/regtest

### Dependencies:

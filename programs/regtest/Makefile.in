DEFS      = -DWINELIB
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none
PROGRAMS  = regtest
ALL_LIBS  = $(WINELIB) $(X_LIBS) $(XLIB) $(LIBS)

C_SRCS = regtest.c

SPEC_SRCS = regtest.spec

all: $(PROGRAMS)

@MAKE_RULES@

regtest: $(OBJS)
	$(CC) -o regtest $(OBJS) $(LDOPTIONS) $(ALL_LIBS)

install::
	$(INSTALL_PROGRAM) regtest $(bindir)/regtest

uninstall::
	$(RM) $(bindir)/regtest

### Dependencies:

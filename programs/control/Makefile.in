DEFS      = -DWINELIB
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none
PROGRAMS  = control
ALL_LIBS  = $(WINELIB) $(X_LIBS) $(XLIB) $(LIBS)

C_SRCS = control.c

all: $(PROGRAMS)

@MAKE_RULES@

control: $(OBJS) $(WINESTUB)
	$(CC) -o control $(OBJS) $(LDOPTIONS) $(ALL_LIBS)

install::
	$(INSTALL_PROGRAM) control $(bindir)/control

uninstall::
	$(RM) $(bindir)/control

### Dependencies:

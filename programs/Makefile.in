MODULE    = none

SUBDIRS = \
	avitools \
	clock \
	cmdlgtst \
	control \
	notepad \
	osversioncheck \
	progman \
	regapi \
	regedit \
	regsvr32 \
	regtest \
	uninstaller \
	view \

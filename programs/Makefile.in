TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none

SUBDIRS = \
	avitools \
	clock \
	cmdlgtst \
	control \
	osversioncheck \
	notepad \
	progman \
	regapi \
	regtest \
	uninstaller \
	view \
	wcmd \
	winemine \
	winhelp \
	winver

all: $(SUBDIRS)

@MAKE_RULES@

install::
	for i in $(SUBDIRS); do (cd $$i; $(MAKE) install) || exit 1; done

uninstall::
	for i in $(SUBDIRS); do (cd $$i; $(MAKE) uninstall) || exit 1; done


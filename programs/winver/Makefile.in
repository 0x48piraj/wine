DEFS      = -DWINELIB
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none
PROGRAMS  = winver
ALL_LIBS  = $(WINELIB) $(X_LIBS) $(XLIB) $(LIBS)

C_SRCS = winver.c

SPEC_SRCS = winver.spec

all: $(PROGRAMS)

@MAKE_RULES@

winver: $(OBJS)
	$(CC) -o winver $(OBJS) $(LDOPTIONS) $(ALL_LIBS)

install::
	$(INSTALL_PROGRAM) winver $(bindir)/winver

uninstall::
	$(RM) $(bindir)/winver

### Dependencies:

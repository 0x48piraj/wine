DEFS      = -DWINELIB
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none
PROGRAMS  = cmdlgtst
ALL_LIBS  = $(WINELIB) $(X_LIBS) $(XLIB) $(LIBS)
WRCEXTRA  = -s -p cmdlgtst

C_SRCS = \
	cmdlgtst.c

SPEC_SRCS = \
	cmdlgtst.spec

RC_SRCS = \
	cmdlgr.rc

all: check_wrc $(PROGRAMS)

@MAKE_RULES@

cmdlgtst: $(OBJS)
	$(CC) -o cmdlgtst $(OBJS) $(LDOPTIONS) $(ALL_LIBS)

install::
	$(INSTALL_PROGRAM) cmdlgtst $(bindir)/cmdlgtst

uninstall::
	$(RM) $(bindir)/cmdlgtst

### Dependencies:

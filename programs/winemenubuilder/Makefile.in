TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = winemenubuilder.exe
APPMODE   = -mwindows
IMPORTS   = ole32
EXTRALIBS = -luuid

C_SRCS = \
	winemenubuilder.c

@MAKE_PROG_RULES@

install::
	$(MKINSTALLDIRS) $(bindir)
	$(INSTALL_SCRIPT) $(TOPSRCDIR)/tools/wineshelllink $(bindir)/wineshelllink

uninstall::
	$(RM) $(bindir)/wineshelllink

### Dependencies:

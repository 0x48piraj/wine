DEFS      = @DLLFLAGS@ -D__WINE__
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
LIBEXT    = @LIBEXT@
MODULE    = winmm
SOVERSION = 1.0
WRCEXTRA  = -s -pwinmm

SPEC_SRCS = \
	mmsystem.spec \
	winmm.spec

C_SRCS = \
	joystick.c \
	lolvldrv.c \
	mci.c \
	mmio.c \
	mmsystem.c \
	time.c

GLUE =  lolvldrv.c \
	mmsystem.c \
	time.c

RC_SRCS= \
	winmm_res.rc

all: check_wrc $ lib$(MODULE).$(LIBEXT)

SUBDIRS = \
	joystick \
	mcianim \
	mciavi \
	mcicda \
	mciseq \
	mciwave \
	midimap \
	wavemap \
	wineoss

$(SUBDIRS): dummy
	@cd $@; $(MAKE)

@MAKE_RULES@

$(RC_SRCS:.rc=.s): $(WRC)

clean::
	$(RM) lib$(MODULE).$(LIBEXT).$(SOVERSION)

### Dependencies:

SPEC_SRCS = ntdll.spec

C_SRCS = \
	critsection.c \
	exception.c \
	error.c \
	file.c \
	misc.c \
	nt.c \
	om.c \
	reg.c \
	rtl.c \
	rtlstr.c \
	string.c \
	sec.c \
	signal_i386.c \
	signal_sparc.c \
	sync.c \
	time.c \
	wcstring.c

all: $(MODULE).o

@MAKE_RULES@

$(MODULE).o: $(OBJS) Makefile.in $(TOPSRCDIR)/Make.rules.in
	$(LDCOMBINE) $(OBJS) -o $@


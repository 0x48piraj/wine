LANGUAGE LANG_SPANISH, SUBLANG_NEUTRAL

STRINGTABLE DISCARDABLE
{
    IDS_HTMLDISABLED    "El renderizado HTML está actualmente deshabilitado."
    IDS_HTMLDOCUMENT    "Documento HTML"
    IDS_DOWNLOADING     "Descargando..."
    IDS_INSTALLING      "Instalando..."
}

ID_DWL_DIALOG DIALOG LOADONCALL MOVEABLE DISCARDABLE  0, 0, 260, 85
STYLE DS_MODALFRAME | DS_CENTER | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION "Instalador de Gecko de Wine"
FONT 8, "MS Shell Dlg"
{
    LTEXT "Está aplicación está intentando mostrar una página HTML. Wine necesita que Gecko (Motor HTML" \
          " de Mozilla) esté instalado para mostrar la página. Pulse Instalar si desea que Wine " \
          "descargue e instale automáticamente Gecko.", ID_DWL_STATUS, 10, 10, 240, 30, SS_CENTER
    CONTROL "Progreso", ID_DWL_PROGRESS, PROGRESS_CLASSA, WS_BORDER|PBS_SMOOTH, 10, 40, 240, 12
    PUSHBUTTON "Cancelar", IDCANCEL, 140, 60, 50, 15, WS_GROUP | WS_TABSTOP
    PUSHBUTTON "Instalar", ID_DWL_INSTALL, 200, 60, 50, 15, WS_GROUP | WS_TABSTOP
}


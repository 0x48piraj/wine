TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = user32
SOVERSION = 1.0
WRCEXTRA  = -w16 -m
ALTNAMES  = user keyboard ddeml display mouse
IMPORTS   = gdi32

C_SRCS = \
	bidi16.c \
	ddeml.c \
	display.c \
	exticon.c \
	mouse.c \
	network.c \
	user_main.c \
	thunk.c

RC_SRCS = \
	disp.rc \
	mouserc.rc

GLUE = thunk.c

EXTRA_OBJS = \
	$(TOPOBJDIR)/controls/controls.o \
	$(TOPOBJDIR)/windows/windows.o

SUBDIRS = \
	$(TOPOBJDIR)/controls \
	$(TOPOBJDIR)/windows

@MAKE_DLL_RULES@

$(EXTRA_OBJS): $(TOOLSUBDIRS) dummy
	@cd `dirname $@` && $(MAKE) `basename $@`


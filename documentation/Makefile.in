TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none

EXTRASUBDIRS = samples

@MAKE_RULES@

install-api-man::
	$(MKINSTALLDIRS) $(mandir)/man$(api_manext)
	for i in man$(api_manext)/*; do $(INSTALL_DATA) $$i $(mandir)/man$(api_manext); done

clean::
	$(RM) -r html api-guide man$(api_manext)


DEFS      = -D__WINE__
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@

PROGRAMS = winebuild
MODULE   = none

C_SRCS = \
	import.c \
	main.c \
	parser.c \
	relay.c \
	res16.c \
	res32.c \
	spec16.c \
	spec32.c \
	utils.c

all: $(PROGRAMS)

@MAKE_RULES@

winebuild: $(OBJS) $(TOPOBJDIR)/libwine_unicode.$(LIBEXT)
	$(CC) $(CFLAGS) -o winebuild $(OBJS) -L$(TOPOBJDIR) -lwine_unicode $(LDFLAGS)

$(TOPOBJDIR)/libwine_unicode.$(LIBEXT):
	cd $(TOPOBJDIR) && $(MAKE) libwine_unicode.$(LIBEXT)

install:: $(PROGRAMS)
	[ -d $(bindir) ] || $(MKDIR) $(bindir)
	$(INSTALL_PROGRAM) winebuild $(bindir)/winebuild

uninstall::
	$(RM) $(bindir)/winebuild

### Dependencies:

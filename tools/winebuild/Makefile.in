DEFS      = -D__WINE__
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@

PROGRAMS = winebuild
MODULE   = none

C_SRCS = \
	import.c \
	main.c \
	parser.c \
	relay.c \
	res16.c \
	res32.c \
	spec16.c \
	spec32.c \
	utils.c

all: $(PROGRAMS)

@MAKE_RULES@

winebuild: $(OBJS)
	$(CC) $(CFLAGS) -o winebuild $(OBJS) $(LIBWINE) $(LIBUNICODE) $(LDFLAGS)

install:: $(PROGRAMS)
	$(MKINSTALLDIRS) $(bindir)
	$(INSTALL_PROGRAM) winebuild $(bindir)/winebuild

uninstall::
	$(RM) $(bindir)/winebuild

### Dependencies:

PROGRAMS = \
	make_xftmpl

MANPAGES = \
	winemaker.de.UTF-8.man.in \
	winemaker.fr.UTF-8.man.in \
	winemaker.man.in

C_SRCS = \
	make_xftmpl.c

IN_SRCS = \
	wineapploader.in

all: wineapploader

.PHONY: install install-dev uninstall

install install-dev:: install-man-pages

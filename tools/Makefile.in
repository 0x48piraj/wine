DEFS      = -D__WINE__ -DNO_TRANSITION_TYPES
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ..
SRCDIR    = @srcdir@
VPATH     = @srcdir@

PROGRAMS = build makedep
MODULE   = none

C_SRCS = build.c makedep.c

all: $(PROGRAMS)

@MAKE_RULES@

build: build.o
	$(CC) $(CFLAGS) -o build build.o

makedep: makedep.o
	$(CC) $(CFLAGS) -o makedep makedep.o


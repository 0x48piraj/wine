DEFS   = -D__WINE__
TOPSRC = @top_srcdir@
MODULE = debugger

C_SRCS = \
	break.c \
	db_disasm.c \
	editline.c \
	hash.c \
	info.c \
	memory.c \
	registers.c \
	stack.c \
	y.tab.c \
	lex.yy.c

all: $(MODULE).o

@MAKE_RULES@

y.tab.c y.tab.h: dbg.y
	$(YACC) -d -t dbg.y

lex.yy.c: debug.l
	$(LEX) -8 -I debug.l

clean::
	$(RM) y.tab.c y.tab.h lex.yy.c

### Dependencies:

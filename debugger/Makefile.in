TOPSRC     = @top_srcdir@
EXTRA_DEFS = -DUSE_READLINE

MODULE = debugger

C_SRCS = \
	break.c \
	db_disasm.c \
	hash.c \
	info.c \
	memory.c \
	registers.c \
	stack.c \
	y.tab.c \
	lex.yy.c

all: $(MODULE).o

@MAKE_RULES@

y.tab.c y.tab.h: dbg.y
	$(YACC) -d -t dbg.y

lex.yy.c: debug.l
	$(LEX) -8 -I debug.l

clean::
	rm -f y.tab.c y.tab.h lex.yy.c


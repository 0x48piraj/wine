DEFS      = -D__WINE__ -DNO_TRANSITION_TYPES
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = debugger

C_SRCS = \
	break.c \
	db_disasm.c \
	editline.c \
	hash.c \
	info.c \
	memory.c \
	registers.c \
	stack.c

GEN_C_SRCS = \
	y.tab.c \
	lex.yy.c

all: $(MODULE).o

@MAKE_RULES@

y.tab.c y.tab.h: dbg.y
	$(YACC) -d -t $(SRCDIR)/dbg.y

lex.yy.c: debug.l
	$(LEX) -8 -I $(SRCDIR)/debug.l


DEFS      = -DWINELIB
TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = none
PROGRAMS  = winedbg
ALL_LIBS  = $(WINELIB) $(X_LIBS) $(XLIB) $(LIBS)
WRCEXTRA  = -A -p $*

C_SRCS = \
	break.c \
	db_disasm.c \
	display.c \
	editline.c \
	expr.c \
	external.c \
	hash.c \
	info.c \
	memory.c \
	module.c \
	msc.c \
	registers.c \
	source.c \
	stabs.c	\
	stack.c \
	types.c \
	winedbg.c

SPEC_SRCS = winedbg.spec

EXTRA_SRCS = dbg.y debug.l
EXTRA_OBJS = y.tab.o lex.yy.o

all: $(PROGRAMS)

depend: y.tab.h

@MAKE_RULES@

y.tab.c y.tab.h: dbg.y
	$(YACC) -d -t $(SRCDIR)/dbg.y

lex.yy.c: debug.l
	$(LEX) -8 -I $(SRCDIR)/debug.l

winedbg: $(OBJS) $(WINESTUB)
	$(CC) -o $@ $(OBJS) $(LDOPTIONS) $(ALL_LIBS)

### Dependencies:
